<h1>New Car</h1>

<%= form_for @car, url:{action: "create"} do |f|%>
<div class="box">
<%= f.label :image%>
<%= f.text_field :img%>


<%= f.label :name%>
<%= f.text_field :name%>


<%= f.label :make%>
<%= f.text_field :make%>


<%= f.label :year%>
<%= f.text_field :year%>


<%= f.label :price%>
<%= f.text_field :price%>


<%= f.label :Phone%>
<%= f.text_field :Phone%>


<%= f.submit "create"%>
</div>
<% end %>




